{
  "name": "sisnodi-backend",
  "version": "1.0.0",
  "description": "Sistema Notariado Digital",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "generar-tests-basicos": "babel-node generar-tests.js",
    "test": "NODE_ENV=test mocha --timeout 15000 test/**/**/*.js",
    "lint": "eslint config/*.js src/*.js src/config/*.js src/models/*.js src/routes/*.js  src/bl/*.js  src/servicios/*.js test/*.js",
    "apidoc": "apidoc -i src/routes/ -o public/apidoc",
    "setup": "FORCE=true babel-node index.js && sequelize db:seed:all --seeders-path src/seeders && sequelize db:migrate:undo:all --migrations-path src/migrations && sequelize db:migrate --migrations-path src/migrations",
    "functions": "sequelize db:migrate:undo:all --migrations-path src/migrations && sequelize db:migrate --migrations-path src/migrations",
    "startdev": "nodemon --exec 'babel-node index.js'",
    "instalar": "npm install",
    "parchar": "babel-node parches/parchar.js"
  },
  "keywords": [
    "node",
    "base",
    "notarias"
  ],
  "apidoc": {
    "name": "API REST - Documentaci√≥n",
    "template": {
      "forceLanguage": "es"
    }
  },
  "author": "AGETIC",
  "license": "MIT",
